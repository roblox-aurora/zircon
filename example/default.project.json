{
    "name": "@zircon/example",
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "Server": {
                "$path": "build/example/server"
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "rbxts_include": {
                "$path": "../include",
                "node_modules": {
                    "$className": "Folder",
                    "zircon": {
                        "$path": "build/src"
                    },
                    "@rbxts": {
                        "$path": "../node_modules/@rbxts"
                    }
                }
            }
        },
        "StarterPlayer": {
            "$className": "StarterPlayer",
            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "Client": {
                    "$path": "build/example/client"
                }
            }
        }
    },
    "globIgnorePaths": [
        "**/package.json"
    ]
}